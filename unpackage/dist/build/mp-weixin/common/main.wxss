page{background:#f5f5f5;font-size:28rpx;line-height:48rpx}uni-button{font-size:32rpx;line-height:50rpx;padding:10rpx 20rpx}uni-button[type=primary]{background-color:#f9506e;border:1px solid #f9506e}uni-button[loading][type=primary],
